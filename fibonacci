class Solution:
    def fib(self, n: int) -> int:
        s = 0
        f = 1
        summ = 0
        #def kem():
            
        if n == 0:
            return 0
        if n ==1:
            return 1
        
        for i in range(n):
            s = f #1,0=1...#1,1=2....#1,2=3...#1,3=4
            f = summ
            summ = s + f
        return summ
