class Solution:
    def isPowerOfFour(self, n: int) -> bool:
        #class Solution:
    #def isPowerOfThree(self, n: int) -> bool:
        if n == 1:
            return True
        x = 1
        #for i in range(n):
        while x < n:
            x*=4
            if x == n:
                return True
